@{
    ViewBag.Title = "Home Page";
}
<div class="col-md-12">
    <div id="renderBody"  class="col-md-5 col-md-offset-1">
        <h2><span class="label label-default">informations</span></h2>
        <form id="form" class="form-horizontal" autocomplete="on" action="" method="POST" accept-charset="UTF-8">
            <fieldset>
                <div class="form-group">
                    <label for="user-input" class="col-sm-3 control-label" accesskey="n">Complete name</label>
                    <div class="col-sm-9">
                        <input type="text" id="user-input" class="form-control" name="nombre" placeholder="Enter your user"
                                autofocus spellcheck="false" required />
                    </div>
                </div>
                <div class="form-group">
                    <label for="password-input" class="col-sm-3 control-label" accesskey="p">Email</label>
                    <div class="col-sm-9">
                        <input type="password" id="password-input" class="form-control" name="password" placeholder="*****" required />
                    </div>
                </div>
            </fieldset>
            <div class="form-group">
                <div class="col-md-offset-8 col-sm-offset-8 col-sm-4">
                    <button type="submit" class="btn btn-primary btn-default btn-block">Add
                        <span data-bind="SetCSSandPopover: 'SS'"></span>
                    </button>
                </div>
            </div>
        </form>
        <div class="bs-example">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Row</th>
                        <th>Complete Name</th>
                        <th>Email</th>
                        <th>Delete</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>1</td>
                        <td>John</td>
                        <td>johncarter@mail.com</td>
                        <td class="glyphicon glyphicon-remove display"></td>
                    </tr>
                    <tr>
                        <td>2</td>
                        <td>Peter</td>
                        <td>peterparker@mail.com</td>
                        <td class="glyphicon glyphicon-remove display"></td>
                    </tr>
                    <tr>
                        <td>3</td>
                        <td>John</td>
                        <td>johnrambo@mail.com</td>
                        <td class="glyphicon glyphicon-remove display"></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>



       
